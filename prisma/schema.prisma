generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Employee {
  id        String @id @default(uuid()) @map("_id") @db.ObjectId
  address   String
  age       String
  firstName String
  lastName  String
  userId    String @db.ObjectId
}

model User {
  id       String @id @default(uuid()) @map("_id") @db.ObjectId
  email    String
  name     String
  password String
}
